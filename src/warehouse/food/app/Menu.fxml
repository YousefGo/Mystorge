<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="800.0" prefWidth="920.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="warehouse.food.app.MenuController">
   <children>
      <VBox prefHeight="83.0" prefWidth="820.0">
         <children>
            <Text fill="#1a2e47" strokeType="OUTSIDE" strokeWidth="0.0" text="Menu">
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
            </Text>
            <Text fill="#1a2e47" strokeType="OUTSIDE" strokeWidth="0.0" text="this page shows the date, time and meal " />
         </children>
      </VBox>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
         <children>
            <JFXComboBox prefHeight="48.0" prefWidth="141.0" promptText="day " />
            <JFXTextField prefHeight="40.0" prefWidth="160.0" promptText="Time" text="Breakfast" />
            <JFXTextField prefHeight="40.0" prefWidth="160.0" promptText="Meal" text="Lunch" />
            <JFXTextField prefHeight="40.0" prefWidth="160.0" promptText="Meal" text="Dinner" />
            <JFXButton graphicTextGap="8.0" prefHeight="40.0" prefWidth="160.0" style="-fx-background-color: #1A2E47;" text="Update" textFill="WHITE">
               <graphic>
                  <FontAwesomeIcon fill="WHITE" iconName="PLUS" size="22" />
               </graphic>
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
      <TableView prefHeight="464.0" prefWidth="820.0" styleClass="resource-table">
         <columns>
            <TableColumn prefWidth="179.0" text="day" />
            <TableColumn minWidth="0.0" prefWidth="96.0" text="breakfast" />
            <TableColumn prefWidth="233.0" text="lunch" />
            <TableColumn prefWidth="194.0" text="dinner" />
         </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
   </padding>
</VBox>
